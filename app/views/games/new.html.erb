<h1>osakedaisuki</h1>
<h3>新規Tweet</h3>

<%= form_for @game do |f| %>
  <div class="field">
    <%= f.label :title %>
    <%= f.text_field :title %>
    <%= f.label :youtube_url %>
    <%= f.text_field :youtube_url, :size => 140 %>
    <%= f.label :body %>
    <%= f.text_field :body, :size => 140 %>
    <div class="check_box">
    <span>タグ</span>
    <%= f.collection_check_boxes(:tag_ids, Tag.all, :id, :name) do |tag| %>
    <div>
      <%= tag.label do %>
        <%= tag.check_box %>
        <%= tag.text %>
      <% end %>
    </div>
  <% end %>
　　</div>
  </div>
  <%= f.submit "投稿する" %>
<% end %>
